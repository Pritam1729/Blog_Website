
<%-include("partials/header");-%>

<h1>Home</h1>

<p><%=homeStarting%></p>
<%posts.forEach(function(ele){%>
    <% var length = 100;
    let subString = (ele.para).substring(0,length);
    %>
    <h1><%= ele.name %></h1>
    <p><%= subString + "..."%><a href = "/posts/<%=ele._id%>" >Read More</a></p>
<%}) %>

