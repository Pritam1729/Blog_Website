
<%-include("partials/header");-%>

<h1>Home</h1>

<p><%=homeStarting%></p>
<%posts.forEach(function(ele){%>
    <% var length = 100;
    let subString = (ele.postText).substring(0,length);
    %>
    <h1><%= ele.title %></h1>
    <p><%= subString + "..."%><a href = "/posts/<%=ele.title%>" >Read More</a></p>
<%}) %>

<%-include("partials/footer");-%>